		"@babel/plugin-proposal-object-rest-spread": "^7.4.0",
		"@babel/preset-env": "^7.4.1",
		"@babel/preset-typescript": "^7.3.3",
		"@types/jasmine": "2.8.7",
		"@types/node": "7.0.0",
		"@typescript-eslint/eslint-plugin": "^4.26.0",
		"@typescript-eslint/parser": "^4.23.0",
		"copy-webpack-plugin": "^8.1.1",
		"css-loader": "^5.2.4",
		"eslint": "^7.26.0",
