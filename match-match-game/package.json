		"karma-jasmine": "^4.0.1",
		"karma-webpack": "^5.0.0-alpha.5",
		"mini-css-extract-plugin": "^1.6.0",
		"source-map-loader": "0.2.3",
		"style-loader": "^2.0.0",
		"ts-loader": "^8.0.12",
		"typescript": "^4.1.3",
